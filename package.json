{
    "name": "demo",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npm run build && node ./bin/www",
        "start-frontend": "NODE_ENV=local npm run build && NODE_ENV=local node ./bin/www",
        "start-backend": "NODE_ENV=local npm run build && NODE_ENV=local nodemon ./bin/www",
        "test": "jest",
        "lint": "./node_modules/.bin/eslint src/**/*.js",
        "build": "./node_modules/.bin/webpack --hot",
        "build-production": "./node_modules/.bin/webpack --mode=production",
        "deploy-stage": "ansible-playbook -i scripts/ansible/inventory.yml scripts/ansible/deploy_stage.yml",
        "deploy-dev": "ansible-playbook -i scripts/ansible/inventory.yml scripts/ansible/deploy_dev.yml",
        "docker-start": "docker-compose up --build",
        "docker-stop": "docker-compose down",
        "docker-reset": "docker-compose down && docker system prune -a && docker volume prune"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "core-js": "^3.2.1",
        "dotenv": "^8.2.0",
        "entities": "^2.0.0",
        "express": "4.17.1",
        "express-session": "^1.16.2",
        "express-validator": "^6.3.1",
        "hbs": "^4.1.0",
        "i18n": "0.8.4",
        "moment": "^2.24.0",
        "mysql": "^2.18.1",
        "request": "^2.88.0",
        "striptags": "^3.1.1",
        "xlsx": "^0.15.3"
    },
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-class-properties": "^7.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-stage-3": "^7.0.0",
        "@blueprintjs/core": "^3.22.0",
        "@material-ui/core": "^4.7.1",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.36",
        "@material-ui/styles": "latest",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.2.0",
        "eslint-plugin-babel": "^5.3.0",
        "file-loader": "^5.0.2",
        "fs": "0.0.1-security",
        "fs-extra": "^8.1.0",
        "jest": "^24.9.0",
        "node-sass": "^4.13.1",
        "nodemon": "^2.0.2",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.18",
        "react-redux": "^7.0.3",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-tag-autocomplete": "^5.11.2",
        "redux": "^4.0.1",
        "sass-loader": "^8.0.0",
        "sinon": "^8.0.4",
        "style-loader": "^1.0.1",
        "webpack": "^4.29.4",
        "webpack-cli": "3.3.10",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    }
}
